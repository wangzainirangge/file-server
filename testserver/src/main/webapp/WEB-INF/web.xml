<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>FileUploadServlet</servlet-name>
    <servlet-class>com.start.demo.servlet.FileUploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileUploadServlet</servlet-name>
    <url-pattern>/FileUploadServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FileDownloadServlet</servlet-name>
    <servlet-class>com.start.demo.servlet.FileDownloadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileDownloadServlet</servlet-name>
    <url-pattern>/FileDownloadServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FileMetadataServlet</servlet-name>
    <servlet-class>com.start.demo.servlet.FileMetadataServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileMetadataServlet</servlet-name>
    <url-pattern>/FileMetadataServlet</url-pattern>
  </servlet-mapping>
</web-app>
